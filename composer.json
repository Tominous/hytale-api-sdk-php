{
    "name": "hytale-game/hytale-api-sdk-php",
    "description": "Hytale API PHP SDK",
    "keywords": [
        "hytale",
        "php",
        "sdk",
        "api"
    ],
    "homepage": "https://hytale.game",
    "license": "MIT",
    "authors": [
        {
            "name": "mTxServ",
            "homepage": "https://mtxserv.com"
        },
        {
            "name": "Hytale.game",
            "homepage": "https://hytale.game"
        }
    ],
    "require": {
        "php": ">=5.5",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "squizlabs/php_codesniffer": "~2.6",
        "friendsofphp/php-cs-fixer": "~2.12"
    },
    "autoload": {
        "psr-4": { "Swagger\\Client\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "Swagger\\Client\\" : "test/" }
    }
}
